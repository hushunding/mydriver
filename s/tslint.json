{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "max-classes-per-file": [],
        "quotemark": false,
        "ordered-imports": false,
        "no-console": [false],
        "no-shadowed-variable": [
            false,
            {
                "class": true,
                "enum": true,
                "function": true,
                "interface": false,
                "namespace": true,
                "typeAlias": false,
                "typeParameter": false
            }
        ],
        "member-ordering":false,
        "max-line-length": [true, 180],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}